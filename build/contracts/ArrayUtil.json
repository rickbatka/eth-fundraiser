  "source": "pragma solidity ^0.4.18;\r\n\r\nlibrary ArrayUtil {\r\n    function contains(address[] vals, address search) public pure returns (bool) {\r\n        for (uint i = 0; i < vals.length; i++) {\r\n            if (vals[i] == search) {\r\n                return true;\r\n            }\r\n        }\r\n        return false;\r\n    }\r\n}",
